#!/usr/bin/env node

var JAWS        = require('../lib/main.js');
var program     = require('commander');

program
    .version(JAWS.version);

program
    .command('new')
    .description('Create a new JAWS project in the current working directory')
    .action(function (prg) {
        JAWS.new(process.cwd());
    });

program.parse(process.argv);
