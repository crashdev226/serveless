{
  "name": "my-project-browserify-tests",
  "version": "1.0",
  "location": "https://github.com/jaws-stack/skinny-jeans",
  "author": "John Serverless <john@gmail.com> http://www.google.com",
  "description": "A lambda function that does something",
  "envVars": [
    "MYAPP_SERVICE_KEY",
    "MYAPP_SERVICE2_KEY"
  ],
  "lambda": {
    "functionName": "browserifytest",
    "runtime": "nodejs",
    "runtimeVer": "0.10.33",
    "handler": "lambdas/browserify/tests/main.run",
    "memorySize": 1024,
    "timeout": 6,
    "deploy": false,
    "package": {
      "optimize": {
        "builder": "browserify",
        "minify": true,
        "ignore": [],
        "exclude": [
          "aws-sdk"
        ]
      },
      "includePaths": [],
      "excludePatterns": []
    }
  }
}