{
  "name": "jaws-users-show",
  "version": "0.0.1",
  "location": "https://github.com/jaws-stack/jaws-users-crud-ddb-jwt-js",
  "author": "JAWS",
  "description": "A lambda function to fetch a user from the database and show them",
  "lambda": {
    "envVars": [
      "MYAPP_SERVICE_KEY",
      "MYAPP_SERVICE2_KEY"
    ],
    "deploy": false,
    "package": {
      "optimize": {
        "builder": "browserify",
        "minify": true,
        "ignore": [],
        "exclude": [
          "aws-sdk"
        ],
        "includePaths": []
      },
      "excludePatterns": []
    },
    "cloudFormation": {
      "Description": "",
      "Handler": "lambdas/sessions/show/index.handler",
      "MemorySize": 1024,
      "Runtime": "nodejs",
      "Timeout": 6
    }
  },
  "apiGateway": {
    "deploy": false,
    "cloudFormation": {
      "path": "sessions/{sessionId}",
      "method": "GET",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestTemplates": {
        "application/json": "{\"access_token\":\"$input.params('access_token')\",\"body\":\"$input.json('$')\"}"
      },
      "requestParameters": {
        "integration.request.querystring.integrationQueryParam": "method.request.querystring.access_token"
      },
      "responses": {
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseTemplates": {
            "application/json": ""
          }
        },
        "400": {
          "statusCode": "400"
        }
      }
    }
  }
}